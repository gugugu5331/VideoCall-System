{
    "ai_detection": {
        "unit_name": "ai-detection",
        "models": {
            "face_swap_detector": {
                "model_path": "./models/face_swap_detector.pb",
                "input_layer": "input_1",
                "output_layer": "output_1",
                "input_shape": [224, 224, 3],
                "detection_threshold": 0.5
            },
            "voice_synthesis_detector": {
                "model_path": "./models/voice_synthesis_detector.pb",
                "input_layer": "input_1", 
                "output_layer": "output_1",
                "input_shape": [128],
                "detection_threshold": 0.5
            }
        },
        "processing": {
            "max_concurrent_tasks": 4,
            "task_timeout_seconds": 300,
            "max_file_size_mb": 100,
            "temp_dir": "/tmp/ai_detection"
        },
        "video_analysis": {
            "sample_rate": 1.0,
            "max_frames": 1000,
            "frame_skip": 30
        },
        "audio_analysis": {
            "sample_rate": 16000,
            "frame_length": 1024,
            "hop_length": 512,
            "n_mfcc": 13,
            "n_fft": 2048
        },
        "content_analysis": {
            "emotion_threshold": 0.6,
            "voice_activity_threshold": 0.5,
            "motion_threshold": 0.3,
            "face_confidence_threshold": 0.9
        },
        "logging": {
            "level": "INFO",
            "log_file": "/var/log/ai_detection.log",
            "max_log_size_mb": 50
        }
    }
}
